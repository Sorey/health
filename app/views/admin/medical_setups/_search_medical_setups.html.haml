- m_iterator = 1
- @medical_setups.each do |m|
  %tr
    %td= m_iterator
    -#%td= m.id.to_s.last(3)
    %td= m.name
    %td= m.address
    %td= m.phone
    %td= m.head_doctor
    %td= m.email
    %td= m.date_start
    %td= m.number_license
    %td= m.date_finish
    %td= m.medical_level
    %td
      = link_to '<i class="fa fa-eye "></i>'.html_safe, admin_medical_setup_path(m), title: "Переглянути",  :class => 'btn btn-info'
      -#= link_to 'Редагувати', edit_admin_news_path(u), :class => 'btn btn-default'
      = link_to '<i class="fa fa-pencil-square-o"></i>'.html_safe, edit_admin_medical_setup_path(m), title: "Редагувати", :class => 'btn btn-primary'
      = link_to '<i class="fa fa-trash-o"></i>'.html_safe, m, title: "Видалити", :method => :delete, :data => { :confirm => 'Ви впевнені?' }, :class => 'btn btn-danger'
    - m_iterator = m_iterator + 1